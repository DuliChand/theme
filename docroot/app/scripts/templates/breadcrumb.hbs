    <li id="breadcrmbsecnd" class="active">{{categoryWrapper.category.categoryName}}</li>
 </ul>
 {{else}}
 <ul class="breadcrumb">
    <li><a href="/">Home</a></li>
    <li id="breadcrmbsecnd" class="active">{{event.eventName}}</li>
 </ul>
 {{/if}}    
{{/if}}