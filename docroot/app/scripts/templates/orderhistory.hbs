				<div id="allorderbox">
					<div id="ordertabs">
						<ul>
						<!-- <li><a href="#" class="active" id="rorders">Recent Orders</a></li>
						 	<li><a href="#" id="porders">Past Orders</a></li> -->
						</ul>
						<div class="clear"></div>
					</div>
					<div class="rorder-table">
						<div class="order-outer">
							<!-- order header starts here -->
							{{#if orders}}
							<div class="order-header">
								<div class="order-no">
									<span>Order No.</span>
								</div>
								<div class="order-no">
									<span>Order Date/Time</span>
								</div>
								<div class="order-items">
									<span>Items</span>
								</div>
								<div class="order-value">
									<span>Order Value</span>
								</div>
							</div>
							<!-- order header ends here -->

							<!-- order content area starts here -->
							{{#if orders.length}} 
							<div class="paging">
							{{#each orders}}
								<div class="order-content">
									
									<div id="last-order" class="content-row">
										<div class="ordno-content">
										<a name="{{orderNumber}}" class="expendable">&nbsp;</a>	<a href="#trackorder1" name="{{orderNumber}}" id="trackorderDetail" class="fancybox button">{{orderNumber}}</a>
										</div>
										<div class="ordvalue-content">
											<span>{{getDOB createDate}} {{getTime 'hh:mm' createDate}}</span>
										</div>
										<div class="orditem-content">
										{{#if_gt totalOrderItems 1}}
											<span>{{totalOrderItems}} Items</span>
										{{else}}
											<span>{{totalOrderItems}} Item</span>
										{{/if_gt}}
										</div>
										<div class="ordvalue-content">
											<span>INR {{totalAmountPaid}}/-</span>
										</div>
										<!--<div class="orddetail">
											<a href="#" name="{{orderNumber}}" class="moredetail">More Info</a>
										</div> -->
									</div>
									<div class="table-outer" id="{{orderNumber}}"></div>
								</div>
								{{/each}}
								</div>
								{{else}}
								<div class="order-content">
									
									<div id="last-order" class="content-row">
										<div class="ordno-content">
										<a name="{{orders.orderNumber}}" class="expendable">&nbsp;</a>	<a href="#trackorder1" id="trackorderDetail" name="{{orders.orderNumber}}" class="fancybox button">{{orders.orderNumber}}</a>
										</div>
										<div class="ordvalue-content">
											<span>{{getDOB orders.createDate}} {{getTime 'hh:mm' createDate}}</span>
										</div>
										<div class="orditem-content">
											<span>{{orders.totalOrderItems}} Items</span>
										</div>
										<div class="ordvalue-content">
											<span>INR {{orders.totalAmountPaid}}/-</span>
										</div>
										<div class="orddetail">
											<a href="#" name="{{orders.orderNumber}}" class="moredetail">More Info</a>
										</div>
									</div>
									<div class="table-outer" id="{{orders.orderNumber}}"></div>
								</div>
							{{/if}}
						</div>
						{{/if}}
						<div class="clear"></div>
					</div>

					<div class="orderTrackDetail">
					<table class="orddetail-table" cellpadding="0" cellspacing="0">
					  <tr class="table table-heading">
						  <th>S. No</th>
						  <th>Product Name</th>
						  <th>Product Code</th>
						  <th>Product Image</th>
						  <th>Status</th>
						  <th>Unit Price</th>
						  <th>Qty.</th>
						  <th>Price</th>
						  <th>Pre Order Amount</th>
						  <th>Subtotal</th>
						 
					  </tr>
		  
					</table>
			</div>
					<div class="porder-table"></div>

					<div class="order-history-popup" id="ordernum1">
						<div class="product-loader"><span></span></div>
					</div>
					<div class="order-history-popup" id="trackorder1"></div>
					
