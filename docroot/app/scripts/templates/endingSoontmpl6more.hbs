{{#if sectionValues}}
<!-- we recommend area starts here -->
<div class="row events {{sectionValues}}">
    <h1 class="col-xs-12">{{sectionValues.title}}</h1>

    {{#if sectionValues.events}}
    <!-- column 1 starts here -->
