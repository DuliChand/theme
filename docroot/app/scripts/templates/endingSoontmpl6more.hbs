        <div class="ftr-2-5">
            <a class="showtooltip"  href="#" title="{{sectionValues.events.0.eventName}}">
                <img alt="{{sectionValues.events.0.eventName}}" src="{{sectionValues.events.0.bannerImage}}"  />
                <div class="tagimage">{{sectionValues.events.0.discountLabel}}</div>
                <h2 class="events-name">{{sectionValues.events.0.eventName}}<span>Ends Today</span></h2>
            </a>
        </div>

        <div class="ftr-3-5">
